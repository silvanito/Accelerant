<%unless @comment_id.blank?%>
  $('<%= escape_javascript("report_comments_#@comment_id")%>').update("<%= escape_javascript(render :partial=>'new', :locals=>{:comment_id => @comment_id, :owner => @owner, :subcomment_id=>@subcomment_id}) %>")
<%else%>
  $('<%= escape_javascript("report_subcomments_#@subcomment_id")%>').update("<%= escape_javascript(render :partial=>'new', :locals=>{:comment_id => @comment_id, :owner => @owner, :subcomment_id=>@subcomment_id}) %>")
<%end%>
